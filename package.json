{
  "name": "eggs",
  "version": "0.1.0",
  "description": "An isomorphic html data binding module, part of the mise.js suite.",
  "repository": {
    "type": "git",
    "url": "https://github.com/misejs/eggs.git"
  },
  "main": "lib/eggs.js",
  "scripts": {
    "test": "mocha --recursive --reporter spec --bail --slow 700 --check-leaks test/lib/",
    "test-browser": "karma start local.karma.conf.js",
    "test-sauce-browser": "[ -e 'sauce_credentials.sh' ] && source sauce_credentials.sh; ./node_modules/.bin/karma start saucelabs.karma.conf.js",
    "test-travis-server": "./node_modules/.bin/mocha --recursive --reporter spec --bail --check-leaks test/lib/",
    "test-travis-browser": "./node_modules/.bin/karma start travis.karma.conf.js"
  },
  "keywords": [
    "data",
    "binding",
    "mise.js"
  ],
  "author": "Jesse Ditson <jesse.ditson@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "observe-js": "^0.4.2"
  },
  "devDependencies": {
    "cheerio": "^0.17.0",
    "jquery": "^2.1.1",
    "karma": "^0.12.24",
    "karma-bro": "^0.10.0",
    "karma-cli": "0.0.4",
    "karma-mocha": "^0.1.9",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-sauce-launcher": "^0.2.10",
    "karma-spec-reporter": "0.0.13",
    "mocha": "^2.0.1"
  }
